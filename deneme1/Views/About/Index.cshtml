@using EntityLayer.Concrete
@model List<About> 
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}


<h2>Hakkımda Veri Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Hakkımda Başlığı</th>
        <th>Aktif yap</th>
        <th>Pasif yap</th>
    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.AboutID</td>
                    <td>Başlık</td>
                    <td><a href="#" class="btn btn-danger">Aktif yap</a></td>
                    <td><a href="#" class="btn btn-dark">Pasif yap</a></td>
                </tr>
            }
</table>

<br/>
@Html.Action("AboutPartial","About") 